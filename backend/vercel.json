{
  "version": 2,
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/items",
      "dest": "dist/app.js"
    },
    {
      "src": "/items/(.*)",
      "dest": "dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}