{
  "version": 2,
  "builds": [
    {
      "src": "app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/items",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/app.ts"
    },
    {
      "src": "/items/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "/app.ts"
    },
    {
      "src": "/db-test",
      "methods": ["GET"],
      "dest": "/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/app.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}